matrix=[[0,1,2,0],[3,4,5,2],[1,3,1,5]]
rows=[0]*len(matrix)
cols=[0]*len(matrix[0])
print(matrix)
for i in range(0,len(matrix)):
    for j in range(0,len(matrix[0])):
        if(matrix[i][j]==0):
            rows[i]=1
            cols[j]=1

for i in range(0,len(matrix)):
    for j in range(0,len(matrix[0])):
        if(rows[i] or cols[j]==1):
            matrix[i][j]=0

        

print(rows)
print(cols)
print(matrix)

